{
  "name": "firebase-react-utils",
  "version": "0.3.8",
  "description": "Firebase react utility functions.",
  "author": "Andrew Han",
  "license": "MIT",
  "main": "build/index.js",
  "scripts": {
    "build": "babel src -x \".ts\" --source-maps --out-dir build && tsc --emitDeclarationOnly",
    "prepare": "npm run build",
    "clean": "rm -rf ./build"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/react": "^18.0.26",
    "babel-cli": "^6.26.0",
    "core-js": "^3.21.1",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "firebase": "^9.6.6",
    "react": "^18.2.0"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "useBuiltIns": "usage",
          "corejs": 3
        }
      ],
      "@babel/react",
      "@babel/preset-typescript"
    ]
  }
}
